{
	"name": "command.ts",
	"version": "1.5.0",
	"description": "A modern discord.js command framework making use of experimental TypeScript decorators",
	"license": "MIT",
	"author": "satoqz",
	"maintainers": [
		"satoqz",
		"C0ffeeCode"
	],
	"bugs": {
		"url": "https://github.com/Satoqz/command.ts/issues"
	},
	"homepage": "https://command.js.org",
	"repository": {
		"type": "git",
		"url": "https://github.com/Satoqz/command.ts.git"
	},
	"keywords": [
		"discord",
		"discordjs",
		"discord.js",
		"command framework",
		"decorators",
		"typescript",
		"commando"
	],
	"main": "./build/lib.js",
	"typings": "./build/lib.d.ts",
	"scripts": {
		"build": "tsc",
		"docgen": "typedoc --out ./docs/ ./src/lib.ts --mode library --excludeNotExported --excludePrivate --stripInternal --hideGenerator",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint src/**/*.ts --fix"
	},
	"dependencies": {
		"discord.js": "^12.2.0"
	},
	"devDependencies": {
		"@types/node": "^14.0.27",
		"@types/ws": "^7.2.6",
		"@typescript-eslint/eslint-plugin": "^3.7.1",
		"@typescript-eslint/parser": "^3.7.1",
		"eslint": "^7.6.0",
		"eslint-config-subaru": "^1.0.5",
		"typedoc": "0.18.0",
		"typescript": "^3.9.7"
	},
	"eslintConfig": {
		"extends": "eslint-config-subaru",
		"rules": {
			"keyword-spacing": [
				"warn",
				{
					"before": true,
					"after": true
				}
			]
		}
	}
}

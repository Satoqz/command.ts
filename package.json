{
	"name": "command.ts",
	"version": "1.7.1",
	"description": "A modern discord.js command framework making use of experimental TypeScript decorators",
	"license": "MIT",
	"author": "satoqz",
	"maintainers": [
		"satoqz",
		"C0ffeeCode"
	],
	"bugs": {
		"url": "https://github.com/Satoqz/command.ts/issues"
	},
	"homepage": "https://command.js.org",
	"repository": {
		"type": "git",
		"url": "https://github.com/Satoqz/command.ts.git"
	},
	"keywords": [
		"discord",
		"discordjs",
		"discord.js",
		"command framework",
		"decorators",
		"typescript",
		"commando"
	],
	"main": "./build/Library.js",
	"typings": "./src/Library.ts",
	"scripts": {
		"build": "tsc",

		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint src/**/*.ts --fix"
	},
	"dependencies": {
		"discord.js": "^12.3.1"
	},
	"devDependencies": {
		"@types/node": "^14.6.0",
		"@types/ws": "^7.2.6",
		"eslint": "^7.7.0",
		"eslint-config-subaru": "^1.1.3",
		"typedoc": "0.17.0-3",
		"typescript": "^4.0.2"
	},
	"eslintConfig": {
		"extends": "eslint-config-subaru"
	}
}
